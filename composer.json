{
    "name": "julienbourdeau/php-v2-poc",
    "description": "POC of a possible v2 for Algolia/algoliasearch-client-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Bourdeau",
            "email": "julien@sigerr.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Algolia\\AlgoliaSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Algolia\\AlgoliaSearch\\Tests\\": "tests/TestCases"
        },
        "files": ["tests/TestCase.php"]
    },
    "require": {
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "^6.5",
        "guzzlehttp/psr7": "^1.0",
        "php-http/message": "^1.6"
    },
    "sort-packages": true
}
